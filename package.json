    "webpack": "5"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "ember-cli-netlify": "^0.4.1",
    "tailwindcss": "^3.3.3"
  }
