    "@glimmer/tracking": "1.1.2",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "autoprefixer": "^10.4.16",
    "broccoli-asset-rev": "3.0.0",
    "ember-a11y-refocus": "3.0.2",
    "ember-auto-import": "~2.6.0",
    "ember-cli": "3.28.6",
    "ember-cli-app-version": "4.0.0",
    "ember-cli-babel": "7.26.11",
